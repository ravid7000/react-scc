{
  "name": "react-scc",
  "version": "0.0.0-beta-2",
  "description": "React Js design pattern for creating components",
  "author": "ravid7000",
  "license": "MIT",
  "repository": "ravid7000/react-scc",
  "main": "index.js",
  "module": "index.es.js",
  "types": "index.d.ts",
  "jsnext:main": "index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "index.*",
    "writable.*",
    "types.d.ts",
    "utils.d.ts"
  ],
  "exports": {
    "./packages.json": "./package.json",
    ".": {
      "import": "./index.es.js",
      "require": "./index.js"
    },
    "./writable": {
      "import": "./writable.es.js",
      "require": "./writable.js"
    }
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir .",
    "start": "rollup -c -w",
    "deploy": "gh-pages -d example/build",
    "format": "eslint --fix",
    "prepublishOnly": "yarn format && yarn build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^2.0.1",
    "react": "^16.9.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^2.26.9",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  }
}