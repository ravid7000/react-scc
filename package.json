{
  "name": "react-scc",
  "version": "0.0.0-beta-3",
  "description": "React.js component building framework",
  "author": "ravid7000",
  "license": "MIT",
  "repository": "ravid7000/react-scc",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "test:watch": "SKIP_PREFLIGHT_CHECK=true react-scripts test --env=jsdom",
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --noEmit false --declarationDir ./dist",
    "start": "rollup -c -w",
    "deploy": "gh-pages -d example/build",
    "format": "eslint --fix",
    "prepare": "yarn build"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^16.9.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^16.9.0",
    "rollup": "^2.26.9",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.0.1",
    "typescript": "^3.2.4"
  }
}
